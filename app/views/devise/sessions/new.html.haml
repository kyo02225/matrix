.devise__form__main
  %ul.devise__form__main__ul
    %h2.log__in__text 
      Log_in
      = link_to new_user_registration_path do
        %button.sign__up Sign_up
    = form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
      %li.devise__form__main__ul__li
        = f.label :nickname
        %br/
        = f.text_field :nickname, autofocus: true, autocomplete: "nickname", class: "devise__form__main__ul__li--text"
      %li.devise__form__main__ul__li
        = f.label :email
        %br/
        = f.email_field :email, autofocus: true, autocomplete: "email", class: "devise__form__main__ul__li--text"
      %li.devise__form__main__ul__li
        = f.label :password
        %br/
        = f.password_field :password, autocomplete: "current-password", class: ".devise__form__main__ul__li--text"
      -# - if devise_mapping.rememberable?
      -#   %li.devise__form__main__ul__li
      -#     = f.check_box :remember_me
          -# = f.label :remember_me
      %li.devise__form__main__ul__li__btn
        = f.submit "Log in"
   